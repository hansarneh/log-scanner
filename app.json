{
  "expo": {
    "name": "Fair Scanner",
    "slug": "fairscanner",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fairscanner.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "permissions": [
        "NSCameraUsageDescription"
      ]
    },
    "android": {
      "package": "com.fairscanner.app",
      "permissions": [
        "android.permission.VIBRATE",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE"
      ],
      "intentFilters": [
        {
          "action": "com.fairscanner.BARCODE",
          "category": [
            "android.intent.category.DEFAULT"
          ],
          "data": {
            "scheme": "fairscanner"
          }
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Tillat appen til å bruke kameraet for å skanne strekkoder."
        }
      ],
      "expo-dev-client"
    ],
    "scheme": "fairscanner",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d1ba345b-c30e-4e17-bcc6-f31458e4afe8"
      }
    },
    "owner": "hansarne"
  }
}
